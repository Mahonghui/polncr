/* CSS Document */

body,h1,p,ul,li{padding:0;margin:0;}
	ul,li{list-style:none;}
	a:visited,a:link{color:#000;text-decoration:none;}
	a:hover{color:#F00;text-decoration:underline;}
	
	body{width:1366px;font-size:12px;font-family:Arial, Helvetica, sans-serif;background-color:#75FFFF;}
	
	.box{width:1200px;margin:0 auto;background-color:#ccc;border-radius:10px;}
	.header{width:1120px;padding:20px 40px;background:url(images/header.jpg) no-repeat;border-radius:10px;}
	.header p{font-size: 18px;font-weight:bold;color:#3C0;font-family:sans-serif;padding-top:40px;}
	.header li{float:left;}
	.header .title{width:580px;height:60px;font-size:26px;color:#0097FE;padding:20px 20px;font-family:Arial,sans-serif;}
	#headerpic {padding-top:10px;}
	.menu{width:1	00%;height:43px;background:url(images/menu.jpg) repeat-x;color:#fff;border-radius:10px;}
	.menu li{float:left;padding:10px 65px;font-weight:bold;font-size:18px;border-right:1px solid #fff;}
	.menu a:link,a:visited{color:#000;}
	.menu a:hover{color:#F00;text-decoration:none;}
	.menu .parent{position:relative;}
	.menu li ul {position:absolute;left:0px;top:40px;}
	.clear{clear:both;}
	
	
	.footer{width:1366px;height:80px;background:#98CDED;font-size:18px;font-family:Arial;margin:0 auto;text-align:center;padding-bottom:50px;color:#000;}
	.footer p{padding:50px;text-align:center;}
	.footer a:hover{text-decoration:none;color:#F9C;}
	.footer a:link,a:visited{color:#000;text-decoration:none;}
